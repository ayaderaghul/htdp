#lang racket
(require test-engine/racket-tests)

; str los -> boolean
(define (contain? str los)
  (cond
    [(empty? los) #f]
    [(cons? los)
     (or
      (string=? str (first los))
      (contain? str (rest los)))]))

(check-expect (contain? "a" (cons "b" '())) #f)
(check-expect (contain? "a" (cons "a" '())) #t)
(test)
